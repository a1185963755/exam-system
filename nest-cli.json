{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/exam-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc",
    "webpack": true,
    "tsConfigPath": "apps/exam-system/tsconfig.app.json"
  },
  "generateOptions": {
    "spec": false
  },
  "monorepo": true,
  "root": "apps/exam-system",
  "projects": {
    "analysis": {
      "type": "application",
      "root": "apps/analysis",
      "entryFile": "main",
      "sourceRoot": "apps/analysis/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analysis/tsconfig.app.json"
      }
    },
    "answer": {
      "type": "application",
      "root": "apps/answer",
      "entryFile": "main",
      "sourceRoot": "apps/answer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/answer/tsconfig.app.json"
      }
    },
    "email": {
      "type": "library",
      "root": "libs/email",
      "entryFile": "index",
      "sourceRoot": "libs/email/src",
      "compilerOptions": {
        "tsConfigPath": "libs/email/tsconfig.lib.json"
      }
    },
    "exam": {
      "type": "application",
      "root": "apps/exam",
      "entryFile": "main",
      "sourceRoot": "apps/exam/src",
      "compilerOptions": {
        "tsConfigPath": "apps/exam/tsconfig.app.json"
      }
    },
    "exam-system": {
      "type": "application",
      "root": "apps/exam-system",
      "entryFile": "main",
      "sourceRoot": "apps/exam-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/exam-system/tsconfig.app.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}